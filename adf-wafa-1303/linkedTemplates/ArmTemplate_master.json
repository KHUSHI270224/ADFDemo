{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-wafa-1303"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"LS_GithubAdf_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_GithubAdf'"
		},
		"Parameterized_LS_SQLDbs_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Parameterized_LS_SQLDbs'"
		},
		"Parameterized_LS_SQLDbs_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "MyDB_server"
		},
		"Parameterized_LS_SQLDbs_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbname}"
		},
		"Parameterized_LS_SQLDbs_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-wafa-1303_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LS_GithubAdf_connectionString": {
						"value": "[parameters('LS_GithubAdf_connectionString')]"
					},
					"Parameterized_LS_SQLDbs_password": {
						"value": "[parameters('Parameterized_LS_SQLDbs_password')]"
					},
					"Parameterized_LS_SQLDbs_properties_typeProperties_server": {
						"value": "[parameters('Parameterized_LS_SQLDbs_properties_typeProperties_server')]"
					},
					"Parameterized_LS_SQLDbs_properties_typeProperties_database": {
						"value": "[parameters('Parameterized_LS_SQLDbs_properties_typeProperties_database')]"
					},
					"Parameterized_LS_SQLDbs_properties_typeProperties_userName": {
						"value": "[parameters('Parameterized_LS_SQLDbs_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}